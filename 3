#include <iostream>
 const int Max = 100;
 using namespace std;
 
 int nhap(int a[], int n){
 	for(int i=0; i<n; i++){
 		cout<<"Nhap vao a["<<i<<"]= ";
 		cin>>a[i];
	 }
 }
 
 int xuat(int a[],  int n){
 	cout<<"Vay mang la: ";
 	for(int i=0; i<n; i++){
 		cout<<a[i]<<" ";
	 }
 }
 
 void doi_cho_truc_tiep(int a[], int n){
 	for(int i=0; i<n-1; i++){
 		for(int j=i+1; j<n;j++){
 			if(a[i] > a[j]){
 				swap(a[i],a[j]);
			 }
		 }
	 }
 }
 
 int main(){
 	int n; cout<<"Nhap n: "; cin>>n;
 	int a[Max];
 	
 	nhap(a,n);
 	xuat(a,n);
 	
 	doi_cho_truc_tiep(a,n);
 	cout<<endl;
 	xuat(a,n);
 	
 	return 0;
 }
